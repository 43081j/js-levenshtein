{
  "name": "js-levenshtein-esm",
  "version": "1.1.6",
  "description": "The most efficient JS implementation calculating the Levenshtein distance, i.e. the difference between two strings.",
  "license": "MIT",
  "repository": "43081j/js-levenshtein",
  "author": "James Garbutt (https://github.com/43081j)",
  "scripts": {
    "clean": "rimraf lib/",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "ava \"test/index.js\"",
    "test:perf": "cd perf && npm i && npm run build && clear && npm test"
  },
  "main": "lib/index.js",
  "files": [
    "lib/index.js",
    "lib/index.d.ts"
  ],
  "ava": {
    "require": [
      "esm"
    ]
  },
  "keywords": [
    "levenshtein",
    "distance",
    "algorithm",
    "algo",
    "string",
    "difference",
    "diff",
    "fast",
    "fuzzy",
    "similar",
    "similarity",
    "compare",
    "comparison",
    "edit",
    "text",
    "match",
    "matching"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "ava": "^1.2.1",
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "esm": "^3.2.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3"
  }
}
